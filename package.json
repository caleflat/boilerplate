{
  "name": "boilerplate",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "nodemon ./src/index.ts",
    "lint": "prettier --config .prettierrc.json './src/**/*.ts' --write",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build:dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "docs": "typedoc src/index.ts"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-jest": "^28.1.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {}
}
